#include <stdio.h>
#include <math.h>

int main() {
    double number, squareRoot;

    // Input from user
    printf("Enter any number: ");
    scanf("%lf", &number);

    // Check for negative input
    if (number < 0) {
        printf("Square root of a negative number is not real.\n");
    } else {
        squareRoot = sqrt(number);
        printf("Square root of %.2lf = %.2lf\n", number, squareRoot);
    }

    return 0;
}
